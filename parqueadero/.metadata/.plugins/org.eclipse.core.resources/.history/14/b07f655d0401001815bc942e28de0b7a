package co.ceiba.parking.modelo.servicio;

import java.util.Scanner;

public class Inicio {

	public static void main(String[] args) {

		Scanner entradaEscaner = new Scanner(System.in);

		System.out.println("Ingresar que tipo de vehiculo a ingresar carro o moto");
		String vehiculo = entradaEscaner.next();
		
		public Vehiculo[] cuposCarro = new Vehiculo[20];
		public Vehiculo[] moto = new Vehiculo[10];
		
		if (vehiculo == "carro") {
			for (saldoCarro = 1; saldoCarro.length < 21; saldoCarro++) {
				vehiculo = entradaEscaner.next();
				vehiculo = vehiculo + saldo;
				System.out.println("fue ingresado un carro " + saldoCarro);
			}
		}
		if (vehiculo == "moto") {
			for (vehiculo = "moto"; saldo < 11; saldo++) {

				System.out.println("favor ingresar para moto");
				vehiculo = entradaEscaner.next();
				vehiculo = vehiculo + saldo;
				System.out.println("motos ingresados" + saldo);

			}

		}
	}
}
