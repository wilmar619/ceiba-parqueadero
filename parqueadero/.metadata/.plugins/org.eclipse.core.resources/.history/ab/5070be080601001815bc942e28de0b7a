		Vehiculo[] moto = new Vehiculo[10];

		if (vehiculo == "carro") {
			for (int i = 1; cuposCarro.length < 21; i++) {
				// cuposCarro;
				System.out.println("fue ingresado un carro " + cuposCarro);
			}
		}
	}
}
